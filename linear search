#include <iostream>
using namespace std;

int main() {
    int n,c;
    cin >> n;
    int b[1000];
    for(int i=0;i<n;i++){
        cin >>b[i];
    }
   for (int i=0;i<n;i++)
  
   cout << b[i] << endl;
   int x;
   cin >> x;
   for (int i=0;i<n;i++)
   {
       if (x==b[i]){
       c= i;
       break;
   }
   }
  
       cout << c << endl;
      
   
   return 0;
}
